{
  "name": "type-regi",
  "version": "1.1.3",
  "description": "event emitter with typescript typing.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "keywords": [
    "TypeScript"
  ],
  "files": [
    "dist"
  ],
  "author": "Fujisawa Shin",
  "license": "ISC",
  "dependencies": {
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.7",
    "jest": "^29.4.3",
    "ts-jest": "^24.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.spec.+(ts|tsx|js)"
    ]
  }
}
